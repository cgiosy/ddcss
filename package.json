{
	"name": "ddcss",
	"version": "0.4.1",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"files": [
		"dist/index.js",
		"dist/index.d.ts"
	],
	"devDependencies": {
		"esbuild": "^0.14.39",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"xxh32": "^1.3.1"
	},
	"scripts": {
		"build": "run-p build:*",
		"build:types": "tsc --noEmit false --emitDeclarationOnly --outDir dist",
		"build:bundle": "esbuild src/index.ts --bundle --format=esm --target=es2019 --external:xxh32 --minify --outfile=dist/index.js",
		"build:bundle-all": "esbuild src/index.ts --bundle --format=esm --target=es2019 --minify --outfile=dist/index.bundle.js",
		"prepack": "run-s build"
	},
	"author": "cgiosy",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/cgiosy/ddcss.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@7.0.0",
	"homepage": "https://github.com/cgiosy/ddcss",
	"keywords": [
		"css",
		"css-in-js",
		"js",
		"styled"
	]
}
